uppercase;\n  font-size: 1.125rem;\n  padding: 0.5em 1.25em;\n  background: var(--clr-accent, #1c69d4);\n  border-radius: 0.25em;\n  transition: transform 250ms ease-in-out, opacity 250ms linear;\n}\n\n.btn:hover,\n.btn:focus {\n  transform: scale(1.1);\n  opacity: 0.9;\n}\n\n.primary-title {\n  font-size: 4rem;\n  font-size: clamp(3rem, calc(5vw + 1rem), 4.5rem);\n  line-height: 1;\n  text-transform: uppercase;\n}\n\n.hero {\n  color: #fff;\n  text-align: center;\n  padding: 15em 0;\n  background: #666;\n\n  @supports (background-blend-mode: multiply) {\n    background: url('../imgs/3_series_bg_2.jpg'), radial-gradient(#bbb, #666);\n    background-blend-mode: multiply;\n    background-attachment: fixed;\n    background-repeat: no-repeat;\n    background-position: center center;\n    background-size: cover;\n  }\n}\n\n.section-title {\n  text-align: center;\n  font-size: clamp(2.5rem, calc(5vw+1rem), 4rem);\n  line-height: 1;\n  color: #17353d;\n  margin-bottom: 5rem;\n}\n\n.split {\n  display: flex;\n  gap: 1em;\n  flex-wrap: wrap;\n  justify-content: center;\n\n  & > * {\n    flex-basis: 30%;\n    min-width: 15em;\n  }\n}\n\n.featured {\n  background: #eee;\n\n  &__item {\n    display: block;\n    position: relative;\n    transform: scale(0.85);\n    transition: tramsform 250ms ease-in-out;\n    text-decoration: none;\n    color: #333;\n    text-align: center;\n    line-height: 1.2;\n\n    &:hover,\n    &:focus {\n      transform: scale(1);\n\n      .featured__details {\n        opacity: 1;\n        text-shadow: 0 0 2em rgba($color: #fff, $alpha: 1);\n      }\n    }\n  }\n\n  &__details {\n    opacity: 0;\n    transition: opacity 250ms linear;\n    span {\n      display: block;\n      font-weight: 700;\n      font-size: 2.5rem;\n    }\n  }\n}\n\n.product {\n  background: radial-gradient(\n    var(--clr-inner, limegreen),\n    var(--clr-outer, purple)\n  );\n  padding: 3em;\n  border-radius: 1em;\n  margin-bottom: 5em;\n  text-align: center;\n\n  &__title {\n    font-size: clamp(3rem, calc(5vw+1rem), 5.5rem);\n    text-transform: uppercase;\n    line-height: 1;\n    color: #fff;\n    text-shadow: 0 0 0.2em rgba($color: #000, $alpha: 0.2);\n  }\n\n  &__image {\n    margin: -5em 0 0 0;\n  }\n\n  @media (min-width: 45em) {\n    text-align: left;\n    margin-bottom: 9em;\n\n    &__title {\n      margin: 0;\n    }\n\n    &__image {\n      float: right;\n      width: 75%;\n      shape-outside: ('../imgs/m2_coupe.png');\n      shape-margin: 1em;\n      margin: 0 -5em 0 0;\n    }\n\n    &.car-left {\n      .product__image {\n        float: left;\n        shape-outside: ('../imgs/m5_sedan.png');\n        shape-margin: 2em;\n        margin: 0 0 0 -5em;\n      }\n    }\n  }\n}\n\nfooter {\n  color: #262626;\n  background: #fbfbfb;\n  text-align: center;\n  height: 3em;\n  line-height: 3em;\n  font-size: 0.9rem;\n  .right {\n    float: right;\n    color: #bbb;\n    overflow: hidden;\n    a {\n      text-decoration: none;\n      color: #1366d7;\n      font-weight: 700;\n    }\n    span {\n      margin-right: 1em;\n    }\n  }\n}\n\n.hero {\n  --clr-accent: #faa700;\n}\n\n.car-red {\n  // --clr-inner: #faa700;\n  // --clr-outer: #e48d00;\n  // --clr-accent: #a1173c;\n  --clr-inner: #d8d3cd;\n  --clr-outer: #797a7e;\n  --clr-accent: #1c69d4;\n}\n\n.car-white {\n  // --clr-inner: #fce4b3;\n  // --clr-outer: #eac886;\n  // --clr-accent: #2f4858;\n  --clr-inner: #d6e0f0;\n  --clr-outer: #8d93ab;\n  --clr-accent: #1c69d4;\n}\n\n.car-blue {\n  --clr-inner: #4e89ae;\n  --clr-outer: #43658b;\n  --clr-accent: #1c69d4;\n}\n"},"lineCount":null}},"error":null,"hash":"fd5a9981d8e4988ca3c0d4052e6c104f","cacheData":{"env":{}}}